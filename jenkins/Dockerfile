FROM python:latest as auto_db

ADD jenkins/autodb.py /opt/autodb.py
ADD E10CN /opt/E10CN
ADD E10TW /opt/E10TW
#ARG TARGET
ARG MODULE
ARG tenantId
ARG tenantSid
RUN echo "MODULE: ${MODULE}"
RUN echo "tenantId: ${tenantId}"
RUN echo "tenantSid: ${tenantSid}"


CMD ["python3", "autodb.py", "$MODULE", "$tenantId", "$tenantSid"]
